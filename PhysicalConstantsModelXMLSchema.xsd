    <xs:include schemaLocation="https://raw.githubusercontent.com/EARTHTIME/Schema/master/ValueModelXMLSchema.xsd"/>
  
    <xs:element name="PhysicalConstantsModel" type="PhysicalConstantsModel"/>
  
    <xs:complexType name="PhysicalConstantsModel">
        <xs:sequence>
            <xs:element name="modelName" type="xs:string" />
            <xs:element name="versionNumber" type="xs:integer" />
            <xs:element name="minorVersionNumber" type="xs:integer" minOccurs="0" maxOccurs="1"/>
            <xs:element name="labName" type="xs:string" />
            <xs:element name="dateCertified" type="xs:date" />
            <xs:element name="reference" type="xs:string" />
            <xs:element name="comment" type="xs:string" />
            <xs:element name="ratios">
                <xs:complexType>
                    <xs:sequence minOccurs="6" maxOccurs="6">
                        <xs:element name="ValueModelReferenced" type="MeasuredConstant">
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="rhos">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="0" maxOccurs="3" name="entry">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="string" type="PhysicalConstantsModelRhoNameType" />
                                    <xs:element name="big-decimal" type="xs:string" />
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="atomicMolarMasses">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element minOccurs="7" maxOccurs="7" name="entry">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="string" type="PhysicalConstantsModelAtomicMolarMassNameType" />
                                    <xs:element name="big-decimal" type="xs:string" />
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>           
            
        </xs:sequence>
    </xs:complexType>
    
    <xs:simpleType name="PhysicalConstantsModelRhoNameType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="rhoLambda230__lambda231" />
            <xs:enumeration value="rhoLambda230__lambda232" />
            <xs:enumeration value="rhoLambda230__lambda235" />
            <xs:enumeration value="rhoLambda230__lambda238" />
            
            <xs:enumeration value="rhoLambda231__lambda232" />
            <xs:enumeration value="rhoLambda231__lambda235" />
            <xs:enumeration value="rhoLambda231__lambda238" />
           
            <xs:enumeration value="rhoLambda232__lambda235" />
            <xs:enumeration value="rhoLambda232__lambda238" />
                      
            <xs:enumeration value="rhoLambda235__lambda238" />

        </xs:restriction>
    </xs:simpleType>    
    
       <xs:simpleType name="PhysicalConstantsModelAtomicMolarMassNameType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="gmol204" />
            <xs:enumeration value="gmol205" />
            <xs:enumeration value="gmol206" />
            <xs:enumeration value="gmol207" />           
            <xs:enumeration value="gmol208" />
            <xs:enumeration value="gmol235" />
            <xs:enumeration value="gmol238" />
        </xs:restriction>
    </xs:simpleType>    


</xs:schema>